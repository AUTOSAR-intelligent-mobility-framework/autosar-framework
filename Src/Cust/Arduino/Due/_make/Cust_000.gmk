
PROJECT   = Arduino

CC_VERSION = 4.9.3

FILES_PRJ = 

SUFFIX_OBJ = o
SUFFIX_ASM = s
SUFFIX_ABS = elf
SUFFIX_LDF = ld
SUFFIX_HEX = hex

TYP_MCAL      = AT91SAM3X8E
TYP_TARGET    = ARDUINO_DUE
TYP_OS        = OS8

TYP_MCAL_THIS = $(TYP_MCAL)
TYP_OS_THIS   = $(TYP_OS)

ifneq ($(TYP_TARGET),$(TYP_TARGET_THIS))
  TYP_MCAL_THIS  = $(TYP_MCAL)_WIN
  TYP_OS_THIS    = $(TYP_OS)_WIN
endif
