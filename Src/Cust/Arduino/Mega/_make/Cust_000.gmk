
PROJECT   = Mega

CC_VERSION = 4.8.2

FILES_PRJ = 

SUFFIX_OBJ = o
SUFFIX_ASM = s
SUFFIX_ABS = elf
SUFFIX_LDF = ld
SUFFIX_HEX = hex

TYP_MCAL      = ATMEGA2560
TYP_TARGET    = ARDUINO_MEGA
TYP_OS        = OS8

TYP_MCAL_THIS = $(TYP_MCAL)
TYP_OS_THIS   = $(TYP_OS)

ifneq ($(TYP_TARGET),$(TYP_TARGET_THIS))
  TYP_MCAL_THIS  = $(TYP_MCAL)_WIN
  TYP_OS_THIS    = $(TYP_OS)_WIN
endif
