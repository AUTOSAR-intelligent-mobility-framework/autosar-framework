
PROJECT   = Labor_Vehicle

CC_VERSION = 4.9.3

FILES_PRJ = 

SUFFIX_OBJ = o
SUFFIX_ASM = s
SUFFIX_ABS = elf
SUFFIX_LDF = ld
SUFFIX_HEX = hex

TYP_MCAL      = STM32F407
TYP_TARGET    = STM32F407_DISCO
TYP_OS        = OS8

TYP_MCAL_THIS = $(TYP_MCAL)
TYP_OS_THIS   = $(TYP_OS)

ifneq ($(TYP_TARGET),$(TYP_TARGET_THIS))
  TYP_MCAL_THIS  = $(TYP_MCAL)_WIN
  TYP_OS_THIS    = $(TYP_OS)_WIN
endif
